<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton3"
            style="@style/Widget.AppCompat.ImageButton."
            android:layout_marginTop="8dp"
            android:text="한식"
            app:layout_constraintEnd_toStartOf="@+id/imageButton4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:drawableTop="@drawable/rice" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton4"
            style="@style/Widget.AppCompat.ImageButton."
            android:text="중식"
            app:layout_constraintEnd_toStartOf="@+id/imageButton5"
            app:layout_constraintStart_toEndOf="@+id/imageButton3"
            app:layout_constraintTop_toTopOf="@+id/imageButton3"
            android:drawableTop="@drawable/lantern" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton5"
            style="@style/Widget.AppCompat.ImageButton."
            android:text="회/스시"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageButton4"
            app:layout_constraintTop_toTopOf="@+id/imageButton4"
            android:drawableTop="@drawable/sushi" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton6"
            style="@style/Widget.AppCompat.ImageButton."
            android:text="치킨"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toStartOf="@+id/imageButton7"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageButton3"
            android:drawableTop="@drawable/fried_chicken" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton7"
            style="@style/Widget.AppCompat.ImageButton."
            android:text="피자"
            app:layout_constraintEnd_toStartOf="@+id/imageButton8"
            app:layout_constraintStart_toEndOf="@+id/imageButton6"
            app:layout_constraintTop_toTopOf="@+id/imageButton6"
            android:drawableTop="@drawable/pizza" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton8"
            style="@style/Widget.AppCompat.ImageButton."
            android:text="햄버거"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageButton7"
            app:layout_constraintTop_toTopOf="@+id/imageButton7"
            android:drawableTop="@drawable/burger" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton9"
            style="@style/Widget.AppCompat.ImageButton."
            android:text="도시락"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toStartOf="@+id/imageButton10"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageButton6"
            android:drawableTop="@drawable/lunch_box" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton10"
            style="@style/Widget.AppCompat.ImageButton."
            android:text="분식집"
            app:layout_constraintEnd_toStartOf="@+id/imageButton11"
            app:layout_constraintStart_toEndOf="@+id/imageButton9"
            app:layout_constraintTop_toTopOf="@+id/imageButton9"
            android:drawableTop="@drawable/shrimp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/imageButton11"
            style="@style/Widget.AppCompat.ImageButton."
            android:text="카페"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageButton10"
            app:layout_constraintTop_toTopOf="@+id/imageButton10"
            android:drawableTop="@drawable/coffee_shop" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
